{
    "openai": {
        "model": "gpt-4o-mini",
        "max_tokens": 1000,
        "temperature": 0.2,
        "response_format": { "type": "json_object" }
    },

    "google_sheet": {
        "functions": {
            "add_transaction": {
                "sheet_name": "items",
                "range": {
                    "total": "B:G",
                    "from": "B",
                    "to": "G"
                }
            },
            "get_balance": {
                "sheet_name": "dashboard",
                "range": {
                    "total": "B3:C7",
                    "from": "B3",
                    "to": "C7"
                }
            },
            "get_payment_methods": {
                "sheet_name": "utils",
                "range": {
                    "total": "C2:C4",
                    "from": "C2",
                    "to": "C4"
                }
            },
            "get_categories": {
                "sheet_name": "utils",
                "range": {
                    "total": "A2:A21",
                    "from": "A2",
                    "to": "A21"
                }
            },
            "build_sheet": {
                "sheet_name": "items",
                "range": {
                    "total": "B1:G1",
                    "from": "B1",
                    "to": "G1"
                }
            },
            "export": {
                "sheet_name": "items",
                "export_mime_type": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
                "range": {
                    "total": "B:F",
                    "from": "B",
                    "to": "F"
                }
            }
        },
        "header": {
            "items": [
                "Data",
                "Metodo",
                "Categoria",
                "Note",
                "Totale"
            ]
        },
        "categories": [
            "Stipendio MS",
            "Mancia",
            "Betting in",
            "Altro in",
            "Rata famiglia",
            "Rata auto / Assicurazione",
            "Benzina",
            "Trasporti",
            "Svago",
            "Cibo",
            "Bar",
            "Vestiti",
            "Ricarica",
            "Abbonamenti",
            "Medicine",
            "Regali",
            "Betting out",
            "Altro out"
        ],
        "payment_methods": [
            "Intesa San Paolo",
            "Buddybank",
            "Contanti"
        ],
        "use_gs": {
            "categories": false,
            "payment_methods": false
        }
    },

    "general": {
        "lang": "it",
        "db": {
            "status": true
        }
    }
}